  "passwordPolicies": [
    {
      "rulename": "uppercase",
      "rule": "Must have 1 Upper Case Alphabet",
      "pattern": "[A-Z]"
    },
    {
      "rulename": "lowercase",
      "rule": "Must have 1 Lower Case Alphabet",
      "pattern": "[a-z]"
    },
    {
      "rulename": "numeral",
      "rule": "Must have 1 Numeral Case Alphabet",
      "pattern": "[0-9]"
    },
    {
      "rulename": "minchar",
      "rule": "Minimum 8 characters required",
      "pattern": "[.*]{8,}"
    },
    {
      "rulename": "maxchar",
      "rule": "Maximum of 50 characters only",
      "pattern": "[.*]{8,50}"
    },
    {
      "rulename": "specialchar",
      "rule": "Must have 1 Special Character [-@#$%^&]",
      "pattern": "[-@#$%^&]"
    },
    {
      "rulename": "moreThanThreeSameNumber",
      "rule": "Must not have more than 3 same number",
     "pattern": "(?!.*?([0-9])\\1{3})([0-9]){3,50}"
    },
    {
      "rulename": "moreThanThreeSameLowerCaseCharacter",
      "rule": "Must not have more than 3 same Lower Case Alphabet",
      "pattern": "(?!.*?([a-z])\\1{3})([a-z]){3,50}"
    },
    {
      "rulename": "moreThanThreeSameUpperCaseCharacter",
      "rule": "Must not have more than 3 same Upper Case Alphabet",
      "pattern": "(?!.*?([A-Z])\\1{3})([A-Z]){3,50}"
    },
    {
      "rulename": "moreThanThreeSameSpecialCharacter",
      "rule": "Must not have more than 3 same Special Character",
      "pattern": "(?!.*?([\\\\#?!@$%^&*-])\\1{3})(?=.*?[\\\\#?!@$%^&*-]){3,50}"
    }
  ]
}
